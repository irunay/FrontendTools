{
    "name": "Frontend Tools",
    "version": "0.1.1",
    "manifest_version": 2,
    "author": "Ray L",
    "homepage_url": "https://github.com/irunay/FrontendTools/",
    "update_url": "http://ftools.sinaapp.com/update.xml",
    "description": "前端开发辅助工具，通过设置浏览器代理使指定域名指向到指定的IP",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "background": {
        "page": "event-page.html",
        "persistent": false
    },
    "browser_action": {
        "default_icon": "img/icon128.png",
        "default_popup": "popup.html"
    },
    "options_page": "options-proxy.html",
    "permissions": [
        "storage",
        "proxy"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
